Xcursor.theme: Bibata-Modern-Ice

*background: #1E272B
*foreground: #EAD49B
*cursorColor: #EAD49B

*color0: #1E272B
*color8: #666666
*color1: #685742
*color9: #685743
*color2: #9D6A47
*color10: #9D6A47
*color3: #B36D43
*color11: #B36D43
*color4: #78824B
*color12: #78824B
*color5: #D99F57
*color13: #D99F57
*color6: #C9A554
*color14: #C9A554
*color7: #EAD49B
*color15: #EAD49B

URxvt.font: xft:Source Code Pro:size=10.5
URxvt.depth:                      32
URxvt*scrollBar:                  false
URxvt*mouseWheelScrollPage:       false
URxvt*cursorBlink:                true
URxvt*saveLines:                  5000
URxvt*internalBorder: 5
URxvt*geometry: 70x19

rofi.color-enabled: true
rofi.color-window: #1E272B, #9D6A47, #9D6A47
rofi.color-normal: #1E272B, #EAD49B, #1E272B, #1E272B, #78824B
rofi.color-active: #1E272B, #EAD49B, #1E272B, #1E272B, #78824B
rofi.color-urgent: #1E272B, #EAD49B, #1E272B, #1E272B, #78824B

rofi.separator-style: solid
rofi.sidebar-mode: false
rofi.lines: 5
rofi.font: Source Code Pro Semibold 10.5
rofi.bw: 1
rofi.columns: 2
rofi.padding: 5
rofi.fixed-num-lines: true
rofi.hide-scrollbar: true

! Normal copy-paste keybindings without perls
URxvt.iso14755:                   false
URxvt.keysym.Shift-Control-V:     eval:paste_clipboard
URxvt.keysym.Shift-Control-C:     eval:selection_to_clipboard
!Xterm escape codes, word by word movement
URxvt.keysym.Control-Left:        \033[1;5D
URxvt.keysym.Shift-Control-Left:  \033[1;6D
URxvt.keysym.Control-Right:       \033[1;5C
URxvt.keysym.Shift-Control-Right: \033[1;6C
URxvt.keysym.Control-Up:          \033[1;5A
URxvt.keysym.Shift-Control-Up:    \033[1;6A
URxvt.keysym.Control-Down:        \033[1;5B
URxvt.keysym.Shift-Control-Down:  \033[1;6B

!!!! UXTerm
UXTerm*foreground:   gray
UXTerm*background:   black
UXTerm*vt0*saveLines:   1000
UXTerm*hideTabbar:  true
UXTerm*scrollBar:   false
!UXTerm*transparent:  false
!UXTerm*transparentScrollbar: false
!UXTerm*transparentTabbar:    false
!UXTerm*transparentMenubar:   false
!UXTerm*textShadow:   white
!UXTerm*textShadowMode:   botright
UXTerm*tabPixmap:   false
UXTerm*syncTabTitle:   true
!UXTerm*bottomTabbar:   false
!UXTerm*windowsSwitch:   false
UXTerm*showMenu:      false
UXTerm*hideButtons:   true
UXTerm*xft:      true
UXTerm*xftAntialias:   true
UXTerm*xftWeight:  small
UXTerm*xftSlant:      roman
UXTerm*xftHinting:   true
UXTerm*xftAutoHint:   true
UXTerm*xftGlobalAdvance:   true
UXTerm*xftWidth:      normal
UXTerm*xftRGBA:      rgb
UXTerm*xftSize:      11
UXTerm*saveLines:         1024
UXTerm*vt0*geometry:          100x30
!UXTerm*boldMode:          false
UXTerm*cutNewline:        false
UXTerm*on4Clicks:         group
UXTerm*on5Clicks:         page
UXTerm*internalBorder:    1
UXTerm*metaSendsEscape:   true
UXTerm*colorBD:           #ffffff
UXTerm*colorBDMode:       false
UXTerm*cursorBlink:       false
UXTerm*cursorColor:       #e2baf1
!UXTerm*veryBoldColors:    14
UXTerm*dynamiccolors: true
UXTerm*utf8: true
! if true, loads .bash_profile. If false, loads .bashrc
UXTerm*loginShell:        true
UXTerm*charClass:         33:48,35:48,37:48,43:48,45-47:48,64:48,95:48,126:48,35:48,58:48,63:48,61:48,44:48,38:48,59:48
UXTerm*jumpScroll:        true
UXTerm*multiScroll:       true


!!!! xft
!Xft.dpi:                    100
Xft.antialias:           true
!Xft.hinting:              full
Xft.hinting:              1
Xft.hintstyle:           hintslight
Xft.rgba:                  rgb


!!!! xmessage
xmessage*form.okay.shapeStyle:          rectangle
xmessage*form.okay.background:          IndianRed
xmessage*form.okay.foreground:          rgb:e/b/b
xmessage*message*background:            ivory3
xmessage*background:                    gray85
xmessage*foreground:                    gray15
xmessage*Scrollbar.width:               1
xmessage*Scrollbar.borderWidth:         0
xmessage*Text.?.cursorColor:            rgb:d/5/5
xmessage*Text.borderColor:              gray65
xmessage*Text*background:               gray95
xmessage*Command.highlightThickness:    1
xmessage*Command.internalWidth:         5
xmessage*Command.internalHeight:        3
xmessage*Command.borderColor:           gray40
xmessage*Command.shapeStyle:            Rectangle
xmessage*Command.background:            gray80
xmessage*font:                          -*-clean-*-*-*-*-*-*-*-*-*-*-iso8859-2
